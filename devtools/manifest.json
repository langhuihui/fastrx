{
    "description": "fastrx debugging tools",
    "manifest_version": 2,
    "name": "fastrx",
    "version": "1.0",
    "author": "dexter",
    "icons": {
        "100": "icons/Rx_Logo_S.png"
    },
    "background": {
        "scripts": [
            "background_scripts/background.js"
        ]
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "devtools_page": "devtools/devtools-page.html",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-script.js"
            ],
            "run_at": "document_start"
        }
    ]
}